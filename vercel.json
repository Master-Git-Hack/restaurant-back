{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 5000",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "main.py"
        },
        {
            "src": "/docs",
            "dest": "main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "main.py"
        }
    ]
}