{
    "devCommand": "uvicorn main:app",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app:main"
        },
        {
            "src": "/docs",
            "dest": "docs/index.html"
        },
        {
            "src": "/openapi.json",
            "dest": "docs/openapi.json"
        }
    ]
}