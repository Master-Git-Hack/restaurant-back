{
    "devCommand": "uvicorn main:app",
    "engines": {
        "node": "18.x"
    },
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "main.py"
        },
        {
            "src": "/docs",
            "dest": "main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "main.py"
        }
    ]
}